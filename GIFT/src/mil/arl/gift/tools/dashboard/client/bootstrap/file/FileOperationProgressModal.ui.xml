<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
 	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
    xmlns:cb='urn:import:mil.arl.gift.common.gwt.client.widgets.bootstrap'>
	<ui:style>
	</ui:style>
	<g:FlowPanel>
	
		 <b:Modal title='' b:id="fileProgressDialog" closable="false" fade="true" dataBackdrop="STATIC" dataKeyboard="true" ui:field="fileProgressDialog">
	        <b:ModalHeader>
	            <b:Heading size="H3">
	                <b.html:Text ui:field='heading'/>
	            </b:Heading>
	        </b:ModalHeader>
	        <b:ModalBody>
	             <b:Row>
	                <b:Column size="MD_12">	                
	                   <g:FlowPanel>
	                   		<cb:ProgressBarListEntry ui:field='progress'/>
	                   </g:FlowPanel>
	                </b:Column>
	            </b:Row>
	        </b:ModalBody>
	        <b:ModalFooter>
	            <b:Button dataDismiss="MODAL" ui:field="hideButton">
	            	Perform in Background
	            </b:Button>
	        </b:ModalFooter>
	   </b:Modal>
		
	</g:FlowPanel>
</ui:UiBinder> 