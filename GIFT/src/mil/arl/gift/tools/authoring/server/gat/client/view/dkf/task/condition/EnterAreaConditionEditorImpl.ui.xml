    xmlns:shared="urn:import:mil.arl.gift.tools.authoring.server.gat.client.view.dkf.shared">
    <ui:style>
        .padTop{
            padding-top: 5px;
        }
        .startLocationEditor {
			padding-top: 10px;
		}
		.teamPicker{
            margin-top: 20px;
        }
