<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	
    <ui:style>
		.wellStyle {
            margin-left:-15px;
            margin-right:-15px;
            margin-bottom: 0px;
            padding-top:10px;
            padding-bottom:10px;
            text-align:center;
            background: linear-gradient(#f0f0f0, #fafafa, #f0f0f0, #d2d2d2 70%, #e6e6e6 90%);
        }
        
        .centerAlign {
           text-align:center;
        }
        
        .rightAlign {
           text-align:right;
        }
        
        .buttonMargin {
           margin-right:10px;
        }
	</ui:style>
    
   
    <b:Container fluid="true">
    
        <b:Well addStyleNames="{style.wellStyle}">
            <b:Button type="PRIMARY" icon="PLUS_CIRCLE" addStyleNames="{style.buttonMargin}" ui:field="addSurveyItemButton">Add Survey Item</b:Button>
            <b:Button type="PRIMARY" icon="PLUS_SQUARE" addStyleNames="{style.buttonMargin}" ui:field="addPageButton">Add New Survey Page</b:Button>
			<b:Button type="PRIMARY" icon="UPLOAD" addStyleNames="{style.buttonMargin}" ui:field="importButton">Import from .qsf</b:Button>
            <!-- <b:Button type="PRIMARY" icon="COPY">Copy Existing Survey Item</b:Button> -->
        </b:Well>
        
        
    </b:Container>
    
</ui:UiBinder> 
