<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
	xmlns:c='urn:import:com.google.gwt.user.cellview.client'>
	<ui:style>
		.modalHeader {
			margin: 4px;
		}
	</ui:style>

	<b:Modal ui:field="rootModal">
		<b:ModalHeader>
			<b:Heading size="H3" text="Current Collaborators" addStyleNames="{style.modalHeader}" />
		</b:ModalHeader>
		<b:ModalBody>
			<g:ScrollPanel>
				<c:CellTable ui:field="accessCellTable" width="99%" />
			</g:ScrollPanel>
		</b:ModalBody>
	</b:Modal>
</ui:UiBinder>