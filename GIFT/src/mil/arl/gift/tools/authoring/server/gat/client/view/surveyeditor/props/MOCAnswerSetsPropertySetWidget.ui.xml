<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:summer="urn:import:org.gwtbootstrap3.extras.summernote.client.ui"
    xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	
    <ui:style>
		.existingCheckbox {
			margin: 4px 0px;
		}
	</ui:style>
    
   
    <b:Container fluid="true">
    
        <b:CheckBox ui:field="existingColumnAnswerSetCheckBox" addStyleNames='{style.existingCheckbox}'>
        	Use Existing Column Answer Set
        </b:CheckBox>
        <b:Collapse toggle="false" ui:field="columnCollapse">
        	<b:ValueListBox ui:field="columnAnswerSetListBox"/>
        </b:Collapse>
        <b:CheckBox ui:field="existingRowAnswerSetCheckBox" addStyleNames='{style.existingCheckbox}'>
        	Use Existing Row Answer Set
        </b:CheckBox>
        <b:Collapse toggle="false" ui:field="rowCollapse">
        	<b:ValueListBox ui:field="rowAnswerSetListBox"/>
        </b:Collapse>
    </b:Container>
    
</ui:UiBinder> 
