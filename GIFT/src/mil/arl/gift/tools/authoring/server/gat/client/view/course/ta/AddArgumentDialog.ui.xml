<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:wb="urn:import:mil.arl.gift.common.gwt.client.widgets.bootstrap">
	<ui:style>
		.pad{
			padding: 10px;
		}
		.padTop{
			padding-top: 5px;
		}
		.largePadTop{
			padding-top: 10px;
		}
		.smallPadBottom{
			margin-bottom: 5px;
		}
		.padBottom{
			margin-bottom: 10px;
		}
		.largePadBottom{
			margin-bottom: 20px;
		}
		.tabbed{
			padding: 0px 30px;
		}
	</ui:style>
	<g:FlowPanel>
	
		<g:InlineHTML addStyleNames='{style.largePadTop}'> 
   			Key Name:
   		</g:InlineHTML> 		
   		<g:FlowPanel addStyleNames='{style.tabbed} {style.padBottom}'>
   			<g:TextBox width='100%' ui:field='nameTextBox'/>
   		</g:FlowPanel>
		
		
		<g:InlineHTML addStyleNames='{style.largePadTop}'> 
   			Value:
   		</g:InlineHTML>
   		<g:FlowPanel addStyleNames='{style.tabbed} {style.padBottom}'>
   			<g:TextBox width='100%' ui:field='valueTextBox'/>
   		</g:FlowPanel>
		
	</g:FlowPanel>
</ui:UiBinder> 