<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
    xmlns:validation="urn:import:mil.arl.gift.common.gwt.client.validation"
    xmlns:list="urn:import:mil.arl.gift.tools.authoring.server.gat.client.view.util.list">
    <ui:style>
        .root-panel {
            padding: 4px;
        }
    </ui:style>
    <g:HTMLPanel>
        <g:FlowPanel addStyleNames='{style.root-panel}'>
            <validation:ValidationWidget ui:field="validationWidget" />
            <list:ItemListEditor ui:field="endTriggersEditor" tableLabel="The assessment will terminate early if any of the following occur:" placeholder="The assessment will only end under normal conditions."/>
        </g:FlowPanel>
    </g:HTMLPanel>
</ui:UiBinder>